]),\n  // computed:{\n  //   allUsers(){\n  //     return this.$store.getters.allUsers;\n  //   }\n  // },\n  methods:{...mapActions(['fetchUsers']),\n    exitDescription(){\n      this.showDescription=false\n    },\n    showerDescription(user){\n    this.showDescription=true\n      this.userName=user.name\n      this.userAddress='USER ADDRESS:\\n '+user.address.street+\n          '\\nSUITE:'+user.address.suite+\n          '\\nCITY:'+user.address.city+\n          '\\nZIPCODE:'+user.address.zipcode\n      console.log(user)\n      document.querySelector('*').style.opacity='0.5'\n      this.$router.push({name:'user',params:{name:user.name}});\n    }\n  },\n  inject:['postList'],\n  mounted() {\n    // this.$store.dispatch('fetchUsers')\n    this.fetchUsers(10)\n  }\n}\n</script>\n\n<style>\n.home{\n  margin-top: 90px;\n}\n.list{\n  border: 2px solid tan;\n  margin-bottom: 15px;\n  cursor: pointer;\n}\n.list:hover{\n  background-color: darkseagreen;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}