{"ast":null,"code":"// @ is an alias to /src\nimport { mapGetters, mapActions } from 'vuex';\nimport UserDescription from \"@/views/UserDescription\";\nexport default {\n  name: 'HomeView',\n  components: {\n    UserDescription\n  },\n\n  data() {\n    return {\n      showDescription: false,\n      userName: '',\n      userStreet: '',\n      userSuite: ''\n    };\n  },\n\n  computed: mapGetters(['allUsers', 'allName']),\n  // computed:{\n  //   allUsers(){\n  //     return this.$store.getters.allUsers;\n  //   }\n  // },\n  methods: { ...mapActions(['fetchUsers']),\n\n    exitDescription() {\n      this.showDescription = false;\n    },\n\n    showerDescription(user) {\n      this.showDescription = true;\n      this.userName = user.name;\n      this.userAddress = 'USER ADDRESS:\\n ' + user.address.street + '\\nSUITE:' + user.address.suite + '\\nCITY:' + user.address.city + '\\nZIPCODE:' + user.address.zipcode;\n      console.log(user);\n      document.querySelector('*').style.opacity = '0.5';\n      this.$router.push({\n        name: 'user',\n        params: {\n          name: user.name\n        }\n      });\n    }\n\n  },\n  inject: ['postList'],\n\n  mounted() {\n    // this.$store.dispatch('fetchUsers')\n    this.fetchUsers(10);\n  }\n\n};","map":{"version":3,"mappings":"AAwBA;AACA,SAAQA,UAAR,EAAmBC,UAAnB,QAAoC,MAApC;AACA,OAAOC,eAAP,MAA4B,yBAA5B;AAEA,eAAe;AAEbC,MAAI,EAAE,UAFO;AAGbC,YAAU,EAAE;AACVF;AADU,GAHC;;AAMbG,MAAI,GAAE;AACJ,WAAM;AACJC,qBAAe,EAAE,KADb;AAEJC,cAAQ,EAAC,EAFL;AAGJC,gBAAU,EAAC,EAHP;AAIJC,eAAS,EAAC;AAJN,KAAN;AAOD,GAdY;;AAebC,UAAQ,EAAEV,UAAU,CAAC,CAAC,UAAD,EAAY,SAAZ,CAAD,CAfP;AAgBb;AACA;AACA;AACA;AACA;AACAW,SAAO,EAAC,EAAC,GAAGV,UAAU,CAAC,CAAC,YAAD,CAAD,CAAd;;AACNW,mBAAe,GAAE;AACf,WAAKN,eAAL,GAAqB,KAArB;AACD,KAHK;;AAINO,qBAAiB,CAACC,IAAD,EAAM;AACvB,WAAKR,eAAL,GAAqB,IAArB;AACE,WAAKC,QAAL,GAAcO,IAAI,CAACX,IAAnB;AACA,WAAKY,WAAL,GAAiB,qBAAmBD,IAAI,CAACE,OAAL,CAAaC,MAAhC,GACb,UADa,GACFH,IAAI,CAACE,OAAL,CAAaE,KADX,GAEb,SAFa,GAEHJ,IAAI,CAACE,OAAL,CAAaG,IAFV,