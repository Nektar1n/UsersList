{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport UserDescription from \"@/views/UserDescription\";\nexport default {\n  name: 'HomeView',\n  components: {\n    UserDescription\n  },\n\n  data() {\n    return {\n      showDescription: false,\n      userDescription: null,\n      userName: ''\n    };\n  },\n\n  computed: mapGetters(['allUsers', 'allName']),\n  methods: { ...mapActions(['fetchUsers']),\n\n    exitDescription() {\n      this.showDescription = false;\n    },\n\n    showerDescription(user) {\n      this.showDescription = true;\n      this.userName = user.name;\n      this.userDescription = user;\n      this.$router.push({\n        name: 'user',\n        params: {\n          id: user.id\n        }\n      });\n    }\n\n  },\n  inject: ['postList'],\n\n  mounted() {\n    this.fetchUsers(10);\n  }\n\n};","map":{"version":3,"mappings":"AA0BA,SAAQA,UAAR,EAAmBC,UAAnB,QAAoC,MAApC;AACA,OAAOC,eAAP,MAA4B,yBAA5B;AAEA,eAAe;AAEbC,MAAI,EAAE,UAFO;AAGbC,YAAU,EAAE;AACVF;AADU,GAHC;;AAMbG,MAAI,GAAE;AACJ,WAAM;AACJC,qBAAe,EAAE,KADb;AAEJC,qBAAe,EAAC,IAFZ;AAGJC,cAAQ,EAAC;AAHL,KAAN;AAKD,GAZY;;AAabC,UAAQ,EAAET,UAAU,CAAC,CAAC,UAAD,EAAY,SAAZ,CAAD,CAbP;AAcbU,SAAO,EAAC,EAAC,GAAGT,UAAU,CAAC,CAAC,YAAD,CAAD,CAAd;;AACNU,mBAAe,GAAE;AACf,WAAKL,eAAL,GAAqB,KAArB;AACD,KAHK;;AAINM,qBAAiB,CAACC,IAAD,EAAM;AACvB,WAAKP,eAAL,GAAqB,IAArB;AACE,WAAKE,QAAL,GAAcK,IAAI,CAACV,IAAnB;AACA,WAAKI,eAAL,GAAqBM,IAArB;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB;AAACZ,YAAI,EAAC,MAAN;AAAaa,cAAM,EAAC;AAACC,YAAE,EAACJ,IAAI,CAACI;AAAT;AAApB,OAAlB;AACF;;AATM,GAdK;AAyBbC,QAAM,EAAC,CAAC,UAAD,CAzBM;;AA0BbC,SAAO,GAAG;AACR,SAAKC,UAAL,CAAgB,EAAhB;AACF;;AA5Ba,CAAf","names":["mapGetters","mapActions","UserDescription","name","components","data","showDescription","userDescription","userName","computed","methods","exitDescription","showerDescription","user","$router","push","params","id","inject","mounted","fetchUsers"],"sourceRoot":"","sources":["/Users/nikitavysokij/Desktop/spa-for-job/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>{{allName}}</h1>\n    <div class=\"all_users\">\n    <div class=\"list_item\" v-for=\"user in allUsers\" :key=\"user.id\" @click=\"showerDescription(user)\">\n      <h1>{{user.name}}</h1>\n      <p>{{user.username}}</p>\n      <p>{{user.email}}</p>\n    </div>\n  </div>\n    <user-description v-if=\"showDescription\" @exitDescription=\"exitDescription\">\n      <template v-slot:name>{{userName}}</template>\n      <template v-slot:street>{{userDescription.address.street}}</template>\n      <template v-slot:suite>{{userDescription.address.suite}}</template>\n      <template v-slot:city>{{userDescription.address.city}}</template>\n      <template v-slot:zipcode>{{userDescription.address.zipcode}}</template>\n      <template v-slot:name_company>{{userDescription.company.name}}</template>\n      <template v-slot:catch_phrase>{{userDescription.company.catchPhrase}}</template>\n      <template v-slot:phone>{{userDescription.phone}}</template>\n      <template v-slot:website>{{userDescription.website}}</template>\n    </user-description>\n  </div>\n</template>\n\n<script>\n\nimport {mapGetters,mapActions} from 'vuex'\nimport UserDescription from \"@/views/UserDescription\";\n\nexport default {\n\n  name: 'HomeView',\n  components: {\n    UserDescription\n  },\n  data(){\n    return{\n      showDescription: false,\n      userDescription:null,\n      userName:'',\n    }\n  },\n  computed: mapGetters(['allUsers','allName']),\n  methods:{...mapActions(['fetchUsers']),\n    exitDescription(){\n      this.showDescription=false\n    },\n    showerDescription(user){\n    this.showDescription=true\n      this.userName=user.name\n      this.userDescription=user\n      this.$router.push({name:'user',params:{id:user.id}});\n    }\n  },\n  inject:['postList'],\n  mounted() {\n    this.fetchUsers(10)\n  }\n}\n</script>\n\n<style>\n.home{\n  margin-top: 90px;\n}\n.all_users{\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n}\n.list_item{\n  border: 2px solid tan;\n  margin-bottom: 15px;\n  cursor: pointer;\n  width: 40%;\n  margin-left: 10px;\n  padding: 10px;\n}\n.list_item:hover{\n  background-color: darkseagreen;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}