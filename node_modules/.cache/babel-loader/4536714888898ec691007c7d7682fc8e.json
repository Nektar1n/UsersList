ist\" v-for=\"user in allUsers\" :key=\"user.id\" @click=\"showerDescription(user)\">\n      <h1>{{user.name}}</h1>\n      <p>{{user.username}}</p>\n      <p>{{user.email}}</p>\n    </div>\n    <user-description v-if=\"showDescription\" @exitDescription=\"exitDescription\">\n        <template v-slot:name>\n            {{userName}}\n        </template>\n      </user-description>\n<!--    <img alt=\"Vue logo\" src=\"../assets/logo.png\">-->\n<!--    <HelloWorld msg=\"Welcome to Your Vue.js App\"/>-->\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport {mapGetters,mapActions} from 'vuex'\nimport UserDescription from \"@/views/UserDescription\";\n\nexport default {\n\n  name: 'HomeView',\n  components: {\n    UserDescription\n  },\n  data(){\n    return{\n      showDescription: false,\n      userName:'',\n      userAdress:''\n    }\n  },\n  computed: mapGetters(['allUsers','allName']),\n  // computed:{\n  //   allUsers(){\n  //     return this.$store.getters.allUsers;\n  //   }\n  // },\n  methods:{...mapActions(['fetchUsers']),\n    exitDescription(){\n      this.showDescription=false\n    },\n    showerDescription(user){\n    this.showDescription=true\n      this.userName=user.name\n    }\n  },\n  inject:['postList'],\n  mounted() {\n    // this.$store.dispatch('fetchUsers')\n    this.fetchUsers(10)\n  }\n}\n</script>\n\n<style>\n.home{\n  margin-top: 90px;\n}\n.list{\n  border: 2px solid tan;\n  margin-bottom: 15px;\n  cursor: pointer;\n}\n.list:hover{\n  background-color: darkseagreen;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}